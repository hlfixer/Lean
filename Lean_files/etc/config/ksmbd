
config globals
	option workgroup 'WORKGROUP'
	option name 'LEDE'
	option description 'LEDE NAS'
	option autoshare '0'

config share
	option browseable 'yes'
	option force_root '1'
	option guest_ok 'yes'
	option create_mask '0666'
	option dir_mask '0777'
	option name 'NAS'
	option path '/mnt/sdb'
	option read_only 'no'

